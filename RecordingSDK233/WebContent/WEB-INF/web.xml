<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>JavaSDK233</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>recordingServlet</servlet-name>
    <servlet-class>io.agora.recording.test.RecordingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>recordingServlet</servlet-name>
    <url-pattern>/record</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>recordLeavechannelServlet</servlet-name>
    <servlet-class>io.agora.recording.test.LeavechannelServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>recordLeavechannelServlet</servlet-name>
    <url-pattern>/leavechannel</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>recordStopServiceServlet</servlet-name>
    <servlet-class>io.agora.recording.test.RecordStopServiceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>recordStopServiceServlet</servlet-name>
    <url-pattern>/stopService</url-pattern>
  </servlet-mapping>
</web-app>